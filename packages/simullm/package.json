{
  "name": "simullm",
  "version": "0.3.0",
  "description": "Event-driven Agent-Based Modeling framework for TypeScript",
  "main": "lib/index.ts",
  "module": "lib/index.ts",
  "types": "lib/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.ts",
      "import": "./lib/index.ts"
    }
  },
  "files": [
    "lib/**/*.ts",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "build:types": "tsc lib/index.ts --declaration --emitDeclarationOnly --outDir dist --target ES2020 --module ESNext --moduleResolution bundler --strict --skipLibCheck",
    "prepublishOnly": "bun run build:types",
    "release": "node scripts/release.js"
  },
  "keywords": [
    "agent-based-modeling",
    "simulation",
    "typescript",
    "event-driven",
    "abm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@openrouter/ai-sdk-provider": "^0.7.3",
    "ai": "^4.3.19",
    "dotenv": "^17.2.1",
    "zod": "^3"
  }
}
